{
  "name": "Youless Chrome App",
  "version": "0.1.0",
  "manifest_version": 2,
  "minimum_chrome_version": "29",
  "app": {
    "background": {
      "scripts": ["js/background.js"]
    }
  },
  "permissions": [
    // this just allows all valid URLS (http, https, ftp, ...)
    // could be limited to http://*/* or https://*/* for security reasons
    "<all_urls>",
    //{"fileSystem": ["write", "retainEntries", "directory"]},
    "storage"
  ]
}
